<h1>Albums for {{ albums.0.artists.0.name }} </h1>
<div class="artists-wrapper">
    {{#each albums}}
        <div class="artist">
        <div class="artist-caption">
            <div class="artist-name">{{ this.name }}</div>
            <div class="artist-viewalbums"><button type="submit" onclick="location.href='/tracks/{{ this.id }}'" class="btn btn-danger">View Tracks</button></div>
        </div>
        {{#if album.images }}
            <img class="artist-image" src="{{this.images.0.url}}" />
        {{else}}
            <img class="artist-image" src="/images/emptyAlbum.png" />
        {{/if}}
        </div>
     {{/each}}
</div>