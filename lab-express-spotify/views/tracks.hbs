<h1>Tracks</h1>
 <table class="table">
    <thead>
      <tr>
        <th>Title</th>
        <th>Listen</th>
      </tr>
    </thead>
    <tbody>
      {{#each tracks}}
      <tr>
        <td>
            {{ this.name }}
        </td>
        <td>
            <audio src="{{ this.preview_url }}" controls>
            Your browser does not support the audio element.
            </audio>
        </td>
      </tr>
      {{/each}}
    </tbody>
</table>


